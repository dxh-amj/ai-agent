  "scripts": {
    "clean:next": "rm -rf frontend/.next",
    "dev": "cd frontend && npx next dev",
    "build": "cd frontend && npx next build",
    "start": "cd frontend && npx next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0 || true",
    "lint:check": "eslint . --max-warnings=0 --quiet",
    "lint:apps": "eslint frontend --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:apps": "prettier --write 'frontend/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
    "code:check": "yarn lint:check && yarn format:check",
    "code:fix": "yarn lint:fix && yarn format",
    "prepare": "husky",
    "commit": "git add . && ./node_modules/.bin/git-cz",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "cd frontend/web-e2e && playwright test",
    "quality:check": "yarn code:check && yarn test:run",
    "quality:fix": "yarn code:fix && yarn test:run",
    "security:check": "yarn audit --level moderate",
    "deps:check": "yarn outdated",
    "deps:update": "yarn upgrade-interactive --latest",
    "validate": "yarn lint:check && yarn format:check && yarn test:run && yarn build"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
